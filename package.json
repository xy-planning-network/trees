{
  "name": "@xy-planning-network/trees",
  "version": "0.6.11",
  "description": "",
  "license": "MIT",
  "repository": "github:xy-planning-network/trees",
  "files": [
    "config/*",
    "dist/*",
    "src/**/*.css",
    "src/**/*.vue",
    "types/*"
  ],
  "main": "dist/trees.es.js",
  "module": "dist/trees.es.js",
  "style": "src/index.css",
  "unpkg": "dist/trees.umd.js",
  "types": "types/entry.d.ts",
  "sideEffects": [
    "*.css",
    "*.vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run lint && vue-tsc -p src --emitDeclarationOnly && vite build && npm run copy:types && tsc-alias -p src/tsconfig.json",
    "build:docs": "vue-tsc -p dev --noEmit && vite build --config vite.docs.config.ts",
    "copy:types": "copyfiles -f ./src/*.d.ts ./types",
    "format": "prettier --write src dev",
    "lint": "eslint --ext .js,.ts,.vue src dev",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src dev",
    "preview": "vite preview --config vite.docs.config.ts",
    "typecheck": "npm run typecheck:src && npm run typecheck:docs",
    "typecheck:src": "vue-tsc -p src --noEmit",
    "typecheck:docs": "vue-tsc -p dev --noEmit"
  },
  "devDependencies": {
    "@point-hub/vue-highlight": "^0.1.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "copyfiles": "^2.4.1",
    "deepmerge": "^4.3.1",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.14.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.5",
    "typescript": "^5.0.4",
    "vite": "^4.3.9",
    "vite-plugin-environment": "^1.1.3",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@floating-ui/vue": "^1.0.1",
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "axios": "^0.27.2",
    "flatpickr": "^4.6.9"
  },
  "peerDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "vue": "^3.3.4"
  },
  "engines": {
    "node": ">=12"
  }
}
