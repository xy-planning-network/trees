{
  "name": "@xy-planning-network/trees",
  "version": "0.6.5-rc-1",
  "description": "",
  "license": "MIT",
  "repository": "github:xy-planning-network/trees",
  "files": [
    "config/*",
    "dist/*",
    "src/**/*.css",
    "src/**/*.vue",
    "types/*"
  ],
  "main": "dist/trees.es.js",
  "module": "dist/trees.es.js",
  "style": "src/index.css",
  "unpkg": "dist/trees.umd.js",
  "types": "types/entry.d.ts",
  "sideEffects": [
    "*.css",
    "*.vue"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc -p src --emitDeclarationOnly && npm run copy:types && tsc-alias -p src/tsconfig.json",
    "build:docs": "vue-tsc -p dev --noEmit && vite build --config vite.docs.config.ts",
    "copy:types": "copyfiles -f ./src/*.d.ts ./types",
    "lint": "eslint --ext .js,.ts,.vue src",
    "lint:fix": "eslint --fix --ext .js,.ts,.vue src dev && prettier -w -u src dev",
    "preview": "vite preview --config vite.docs.config.ts",
    "typecheck": "npm run typecheck:src && npm run typecheck:docs",
    "typecheck:src": "vue-tsc -p src --noEmit",
    "typecheck:docs": "vue-tsc -p dev --noEmit"
  },
  "devDependencies": {
    "@point-hub/vue-highlight": "^0.1.4",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/node": "^16.11.13",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^9.1.0",
    "autoprefixer": "^10.4.13",
    "copyfiles": "^2.4.1",
    "deepmerge": "^4.2.2",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "mitt": "^2.1.0",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.1",
    "tsc-alias": "^1.5.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-environment": "^1.0.1",
    "vue-tsc": "^0.29.8"
  },
  "dependencies": {
    "@headlessui/vue": "^1.4.2",
    "@heroicons/vue": "^1.0.5",
    "axios": "^0.27.2",
    "flatpickr": "^4.6.9"
  },
  "peerDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "vue": "^3.2.0"
  },
  "engines": {
    "node": ">=12"
  }
}
